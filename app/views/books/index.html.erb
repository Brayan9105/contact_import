<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>

<table>
  <tr>
    <th>Filename</th>
    <th>Status</th>
    <th>options</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= link_to book.filename, book_path(book) %></td>
      <td><%= book.status %></td>
      <td>
        <%= link_to 'processing', root_path if book.espera? %>
        <%= link_to 'view', book_path(book) %>
      </td>
    </tr>
  <% end %>
</table>
